{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "import/extensions": [
            "error",
            "ignorePackages"
        ],
        "no-unused-vars": "off",
        "guard-for-in": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "no-plusplus": "off"
    }
}
